<Window x:Class="PrintCrypt.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="250" Width="400">
    <Grid>
        <StackPanel Margin="20">
            <Button Content="Scan QR Code" Click="ScanButton_Click" HorizontalAlignment="Center" Width="150" Margin="0 0 0 10"/>
            <Button Content="Generate Key" Click="GenerateKeyButton_Click" HorizontalAlignment="Center" Width="150"/>
        </StackPanel>
    </Grid>
</Window>

